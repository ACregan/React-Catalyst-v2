{
  "name": "catalyst2",
  "version": "2.2.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-s start-dev-server",
    "start-dev-server": "cross-env NODE_ENV=development babel-node catalyst/dev-server/server.js",
    "prebuild": "run-s bump:patch dist-flush",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "dist-flush": "rimraf ./dist",
    "dist-server": "babel-node ./catalyst/dist-server/server.js",
    "ssr-start": "run-s dist-flush ssr-build ssr-server",
    "ssr-build": "cross-env NODE_ENV=production webpack --config webpack.config.ssr.js",
    "ssr-server": "cross-env NODE_ENV=production babel-node src/ssr-server.js",
    "bump:major": "npm --no-git-tag-version -f version major",
    "bump:minor": "npm --no-git-tag-version -f version minor",
    "bump:patch": "npm --no-git-tag-version -f version patch",
    "test": "jest -c jest.config.js --no-cache",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "test:coverage": "jest -c jest.config.js --coverage",
    "test:staged": "jest -c jest.config.js --findRelatedTests --silent",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:record": "cypress run --record --key YOUR_PROJECT_KEY_HERE",
    "lint": "jest --config testing/jest.lint.js",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.conf.json"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": ".eslintignore"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.{js,jsx}": [
      "npm run test:staged"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
